<script language="javascript">
var clicked = false;
// Shamelessly adapted from http://www.randomsnippets.com/2008/02/21/how-to-dynamically-add-form-elements-via-javascript/ 09-04-14. jh
function addInput(divName,btnName,btnLabel) {
   var newdiv = document.createElement('div');
   newdiv.innerHTML = "<input type='hidden' name='" + btnName +
			"' value='" + btnLabel + "'>";
   divName.appendChild(newdiv);
}
// -- 09-04-14.
function check(form1,formBtn) {
   // if submitted by <enter> key, add a submit button lookalike to prevent
   // 1003 errs from BRS. 9-4-14 jh
   if (!clicked)
      addInput (form1,formBtn.name,formBtn.value);
   return true;
}
function unesc(){
   srchForm.Srch1.value = unescape(srchForm.Srch1.value);
   if (srchForm.Srch2)
      srchForm.Srch2.value = unescape(srchForm.Srch2.value);
}
function unesc2(){
   srchForm2.Srch1.value = unescape(srchForm2.Srch1.value);
   if (srchForm2.Srch2)
      srchForm2.Srch2.value = unescape(srchForm2.Srch2.value);
}
function decod(){
   document.title = decodeURIComponent(document.title);
   refine.Query.value = decodeURIComponent(refine.Query.value);
   var x = document.getElementById("srchtext").childNodes[0];
   x.data = decodeURIComponent(x.data);
}
</script>
</HEAD>
